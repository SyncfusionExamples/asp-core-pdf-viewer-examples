@page "{handler?}"
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div style="width:100%;height:600px">
    <!-- To configure the server-backed PDF Viewer, include the "serviceUrl='/Index'" attribute -->
    <ejs-pdfviewer id="pdfviewer"
                   documentPath="https://cdn.syncfusion.com/content/pdf/pdf-succinctly.pdf"
                   resourceUrl="https://localhost:44386/wwwroot/ej2-pdfviewer-lib"
                   customFonts="customFonts"
                   style="height:641px;">
    </ejs-pdfviewer>
</div>
<script type="text/javascript">
    var customFonts = ["simsun.ttc", "sumsinb.ttf", "arial/arialbd.ttf", "arial/arial.ttf", "BKANT.TTF", "calibri.ttf", "GARA.TTF", "GARAIT.TTF", "msgothic.ttc", "trebuc.ttf", "wingding.ttf"];
</script>
