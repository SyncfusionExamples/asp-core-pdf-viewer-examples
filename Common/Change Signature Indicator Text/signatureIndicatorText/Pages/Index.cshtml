@page "{handler?}"
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <ejs-pdfviewer id="pdfviewer" style="height:600px" serviceUrl="/Index" documentPath="Data/PDF_Succinctly.pdf">
    </ejs-pdfviewer>
</div>


<script>
    window.onload = function () {
        var pdfviewer = document.getElementById('pdfviewer').ej2_instances[0];
        pdfviewer.signatureFieldSettings.signatureIndicatorSettings.text = "Syncfusion";
    }
</script>